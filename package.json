    "electron-devtools-installer": "^2.2.4",
    "electron-download": "^4.1.1",
    "electron-rebuild": "^1.10.0",
    "fbjs-scripts": "^1.2.0",
    "file-loader": "^5.1.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
